"std/stack.stack" import
"std/let.stack" import

'(fn
  0 'i set
  ['list get len] 'length set

  '(fn!
    i length <
  ) 'i-in-bounds set

  '(fn!
    i 1 + 'i set
  ) 'increment-i set

  '(fn!
    list i nth
  ) 'get-nth set

  '(
    get-nth

    block

    increment-i
  )
  '(i-in-bounds) while
) '(list block) 'list/for_each defn