'(fn
  swap
  '(drop)
  '([tolist] ["assertion failed: " tolist] [swap] [concat "" join] panic)
  '(true !=)
  ifelse
) 'assert set

'(fn
  'context set
  'right set
  'left set

  '('() ["assertion for [" push] [context push] ["] failed:\nLeft = " push] [left push] ["\nRight = " push] [right push] ["" join] panic)
  '(left right !=)
  if
) 'assert-eq set
